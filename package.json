{
  "name": "oura-js",
  "version": "0.0.1",
  "description": "Oura Cloud API client for JavaScript",
  "author": "Shinya Fujino <shf0811@gmail.com>",
  "license": "MIT",
  "keywords": [
    "oura ring",
    "oura",
    "api"
  ],
  "homepage": "https://github.com/morinokami/oura.js#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/morinokami/oura.js.git"
  },
  "bugs": {
    "url": "https://github.com/morinokami/oura.js/issues"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll",
    "lint": "eslint 'src/**/*.ts'",
    "validate": "npm-run-all --parallel build test lint",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^27.4.4",
    "lint-staged": "^12.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "simple-git-hooks": "^2.7.0",
    "ts-jest": "^27.1.1",
    "typescript": "^4.5.3"
  },
  "dependencies": {
    "axios": "^0.24.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "prettier --write",
      "eslint"
    ]
  }
